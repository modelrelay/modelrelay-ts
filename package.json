{
	"name": "@modelrelay/sdk",
	"version": "1.15.0",
	"description": "TypeScript SDK for the ModelRelay API",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup src/index.ts --format esm,cjs --dts --external playwright",
		"dev": "tsup src/index.ts --format esm,cjs --dts --watch",
		"lint": "tsc --noEmit --project tsconfig.lint.json",
		"test": "vitest run",
		"generate:types": "openapi-typescript ../../api/openapi/api.json -o src/generated/api.ts"
	},
	"keywords": [
		"modelrelay",
		"llm",
		"sdk",
		"typescript"
	],
	"author": "Shane Vitarana",
	"license": "Apache-2.0",
	"dependencies": {
		"fast-json-patch": "^3.1.1",
		"zod": "^3.23.0"
	},
	"peerDependencies": {
		"playwright": ">=1.40.0"
	},
	"peerDependenciesMeta": {
		"playwright": {
			"optional": true
		}
	},
	"devDependencies": {
		"@types/node": "^25.0.3",
		"openapi-typescript": "^7.4.4",
		"playwright": "^1.49.0",
		"tsup": "^8.2.4",
		"typescript": "^5.6.3",
		"vitest": "^2.1.4"
	}
}
